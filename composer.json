{
    "require": {
        "vlucas/phpdotenv": "1.0.6",
        "twig/twig": "~1.16.0",
        "silex/silex": "~1.2.1",
        "monolog/monolog": "~1.10.0",
        "symfony/twig-bridge": "~2.5.4",
        "symfony/debug": "~2.5.4",
        "guzzlehttp/guzzle": "~4.0.0",
        "psr/log": "*",
        "phpoption/phpoption": "~1.4.0",
        "nesbot/Carbon": "~1.13.0",
        "kriswallsmith/assetic": "~1.1.2",
        "mheap/silex-assetic": "~1.0.4",
        "natxet/CssMin": "~3.0.2",
        "knplabs/github-api": "~1.2",
        "twig/extensions": "~1.1.0",
        "davedevelopment/twig-inflection": "1.0.*@dev",
        "codegyre/robo": "0.4.5",
        "gabrielelana/byte-units": "dev-master",
        "filp/whoops": "~1.1.2",
        "fxp/composer-asset-plugin": "~1.0",
        "bower-asset/octicons": "~2.1.2",
        "bower-asset/font-awesome": "~4.2.0",
        "bower-asset/bootstrap": "~3.0.0",
        "bower-asset/jquery": "~2.1.1",
        "bower-asset/chartjs": "1.0.2",
        "bower-asset/bootstrap-social": "~4.8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3.0",
        "symfony/browser-kit": "~2.5.4",
        "symfony/css-selector": "~2.5.4",
        "codeclimate/php-test-reporter": "dev-master",
        "heroku/heroku-buildpack-php": "~71.0",
        "psy/psysh": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "Purplapp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Purplapp\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "compile": [
            "./bin/robo clean",
            "./bin/robo init",
            "./bin/robo assets"
        ]
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "assets"
        }
    }
}
